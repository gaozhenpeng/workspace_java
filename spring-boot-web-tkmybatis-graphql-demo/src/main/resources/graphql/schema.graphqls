type AuthorDto {
    #作者Id
    id: Long!
    #创建时间
    createdTime: String
    #名
    firstName: String
    #姓
    lastName: String
    #该作者的所有书籍
    books: [BookDto]
}

input BookReq {
    title: String!
    isbn: String!
    pageCount: Int
    authorId: Long
}

type BookDto {
    id: Long!
    title: String!
    isbn: String!
    pageCount: Int
    author: AuthorDto
}